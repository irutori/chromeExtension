{
	"name": "My First Extension",
	"version": "1.2",
	"manifest_version": 2,
	"description": "for idolmaster on chrome",
	"background":{"page": "background.html"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"http://sp.pf.mbga.jp/12008305/*"
			],
			"js": [
				"jquery.min.js",
				"handlepage.js"
			],
			"run_at": "document_end"
		}
	]
}