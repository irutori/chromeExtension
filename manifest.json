{
	"name": "auto cheer",
	"version": "2.5",
	"manifest_version": 2,
	"description": "auto cheer",
	"background":{
		"page": "background.html"
	},
	"permissions": [
		"tabs"
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://sp.pf.mbga.jp/12008305/*"
			],
			"js": [
				"jquery.min.js",
				"handlepage.js"
			],
			"run_at": "document_end"
		}
	]
}