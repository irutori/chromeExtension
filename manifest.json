{
	"name": "My First Extension",
	"version": "1.1",
	"manifest_version": 2,
	"description": "The first extension that I made.",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"http://api.flickr.com/"
	],
	"content_scripts": [
		{
			"matches": [
				"http://sp.pf.mbga.jp/12008305/*"
			],
			"js": [
				"handlepage.js"
			],
			"run_at": "document_end"
		}
	]
}